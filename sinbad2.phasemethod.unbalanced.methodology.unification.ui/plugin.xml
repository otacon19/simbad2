<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="flintstones.phasemethod.ui">
      <ui
            id="flintstones.phasemethod.unbalanced.methodology.unification.ui"
            name="%ui.name"
            phasemethod="flintstones.phasemethod.unbalanced.methodology.unification">
         <view
               step="sinbad2.phasemethod.unbalanced.methodology.unification.ui.view.GenerateLH">
         </view>
         <view
               step="sinbad2.phasemethod.unbalanced.methodology.unification.ui.view.CalculateRepresentation">
         </view>
      </ui>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="flintstones.phasemethod.unbalanced.methodology.unification.ui.category"
            name="%category.name">
      </category>
      <view
            category="flintstones.phasemethod.unbalanced.methodology.unification.ui.category"
            class="sinbad2.phasemethod.unbalanced.methodology.unification.ui.view.GenerateLH"
            id="flintstones.phasemethod.unbalanced.methodology.unification.ui.generatelh"
            name="%view.name"
            restorable="true">
      </view>
      <view
            category="flintstones.phasemethod.unbalanced.methodology.unification.ui.category"
            class="sinbad2.phasemethod.unbalanced.methodology.unification.ui.view.CalculateRepresentation"
            id="flintstones.phasemethod.unbalanced.methodology.unification.ui.calculaterepresentation"
            name="%view.name.0"
            restorable="true">
      </view>
   </extension>

