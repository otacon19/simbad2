   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="sinbad2.element.ui.handler.expert.add.AddExpertHandler"
            commandId="flintstones.element.expert.add">
         <enabledWhen>
            <with
                  variable="flintstones.element.ui.can.be.modified.state">
               <equals
                     value="ENABLED">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.expert.modify.ModifyExpertHandler"
            commandId="flintstones.element.expert.modify">
         <enabledWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        operator="and">
                     <instanceof
                           value="sinbad2.element.expert.Expert">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
               <with
                     variable="flintstones.element.ui.can.be.modified.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.expert.remove.RemoveExpertHandler"
            commandId="flintstones.element.expert.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.expert.Expert">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
               <with
                     variable="flintstones.element.ui.can.be.modified.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.expert.remove.RemoveMultiplesExpertsHandler"
            commandId="flintstones.element.expert.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.expert.Expert">
                     </instanceof>
                  </iterate>
               </with>
               <not>
                  <reference
                        definitionId="oneElementSelected">
                  </reference>
               </not>
               <with
                     variable="flintstones.ui.brothers.experts.selected.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
               <with
                     variable="flintstones.element.ui.can.be.modified.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.alternative.add.AddAlternativeHandler"
            commandId="flintstones.element.alternative.add">
         <enabledWhen>
            <with
                  variable="flintstones.element.ui.can.be.modified.state">
               <equals
                     value="ENABLED">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.alternative.modify.ModifyAlternativeHandler"
            commandId="flintstones.element.alternative.modify">
         <enabledWhen>
            <and>
               <with
                     variable="selection">
                  <iterate>
                     <instanceof
                           value="sinbad2.element.alternative.Alternative">
                     </instanceof>
                  </iterate>
