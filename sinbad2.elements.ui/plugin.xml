                  </equals>
               </with>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.alternative.remove.RemoveAlternativeHandler"
            commandId="flintstones.element.alternative.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.alternative.Alternative">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
               <with
                     variable="flintstones.element.ui.can.be.modified.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.alternative.remove.RemoveMultipleAlternativeHandler"
            commandId="flintstones.element.alternative.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.alternative.Alternative">
                     </instanceof>
                  </iterate>
               </with>
               <not>
                  <reference
                        definitionId="oneElementSelected">
                  </reference>
               </not>
               <with
                     variable="flintstones.element.ui.can.be.modified.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.criterion.add.AddCriterionHandler"
            commandId="flintstones.element.criterion.add">
         <enabledWhen>
            <with
                  variable="flintstones.element.ui.can.be.modified.state">
               <equals
                     value="ENABLED">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.criterion.modify.ModifyCriterionHandler"
            commandId="flintstones.element.criterion.modify">
         <enabledWhen>
            <and>
               <with
                     variable="selection">
                  <iterate>
                     <instanceof
                           value="sinbad2.element.criterion.Criterion">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
               <with
                     variable="flintstones.element.ui.can.be.modified.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.criterion.remove.RemoveCriterionHandler"
            commandId="flintstones.element.criterion.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.criterion.Criterion">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
               <with
                     variable="flintstones.element.ui.can.be.modified.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.criterion.remove.RemoveMultipleCriteriaHandler"
            commandId="flintstones.element.criterion.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.criterion.Criterion">
                     </instanceof>
                  </iterate>
               </with>
               <not>
                  <reference
                        definitionId="oneElementSelected">
                  </reference></not>
               <with
                     variable="flintstones.element.ui.can.be.modified.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
               <with
                     variable="flintstones.ui.brothers.criteria.selected.state">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </and>
         </activeWhen>
      </handler>
